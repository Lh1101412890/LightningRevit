		<Compile Include="..\LightningRevit_V2019\Views\CreatBeamView.xaml.cs" Link="Views\CreatBeamView.xaml.cs" />
		<Compile Include="..\LightningRevit_V2019\Views\CreatWallsColumnsView.xaml.cs" Link="Views\CreatWallsColumnsView.xaml.cs" />
