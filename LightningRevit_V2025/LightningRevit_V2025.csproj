<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>annotations</Nullable>
		<PlatformTarget>x64</PlatformTarget>
		<OutputType>Library</OutputType>
		<UseWPF>True</UseWPF>
		<UseWindowsForms>True</UseWindowsForms>
		<Platforms>x64</Platforms>
		<RootNamespace>LightningRevit</RootNamespace>
		<AssemblyName>LightningRevit</AssemblyName>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<Version>1.3.0</Version>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
		<DebugType>full</DebugType>
		<DefineConstants>$(DefineConstants);R25</DefineConstants>
		<WarningLevel>8</WarningLevel>
		<NoWarn>1701;1702;NU1701</NoWarn>
		<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
		<DebugType>full</DebugType>
		<DefineConstants>$(DefineConstants);R25</DefineConstants>
		<WarningLevel>8</WarningLevel>
		<NoWarn>1701;1702;NU1701</NoWarn>
		<CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
	</PropertyGroup>

	<ItemGroup>
		<Folder Include="Commands\" />
		<Folder Include="Extension\" />
		<Folder Include="LightningExtension\" />
		<Folder Include="Models\" />
		<Folder Include="Views\" />
	</ItemGroup>

	<ItemGroup>
		<Compile Include="..\LightningRevit_V2019\Commands\BackgroundSwitch.cs" Link="Commands\BackgroundSwitch.cs" />
		<Compile Include="..\LightningRevit_V2019\Commands\Views.cs" Link="Commands\Views.cs" />
		<Compile Include="..\LightningRevit_V2019\Extension\BoundingBoxXYZExtension.cs" Link="Extension\BoundingBoxXYZExtension.cs" />
		<Compile Include="..\LightningRevit_V2019\Extension\ImportInstanceExtension.cs" Link="Extension\ImportInstanceExtension.cs" />
		<Compile Include="..\LightningRevit_V2019\Extension\UIApplicationExtension.cs" Link="Extension\UIApplicationExtension.cs" />
		<Compile Include="..\LightningRevit_V2019\Extension\XYZExtension.cs" Link="Extension\XYZExtension.cs" />
		<Compile Include="..\LightningRevit_V2019\LightningApp.cs" Link="LightningApp.cs" />
		<Compile Include="..\LightningRevit_V2019\LightningExtension\ConnectionElementSelectionFilter.cs" Link="LightningExtension\ConnectionElementSelectionFilter.cs" />
		<Compile Include="..\LightningRevit_V2019\LightningExtension\ExternalEventHandler.cs" Link="LightningExtension\ExternalEventHandler.cs" />
		<Compile Include="..\LightningRevit_V2019\LightningExtension\FamilyLoadOptions.cs" Link="LightningExtension\FamilyLoadOptions.cs" />
		<Compile Include="..\LightningRevit_V2019\LightningExtension\Information.cs" Link="LightningExtension\Information.cs" />
		<Compile Include="..\LightningRevit_V2019\Models\BeamModel.cs" Link="Models\BeamModel.cs" />
		<Compile Include="..\LightningRevit_V2019\Models\Column.cs" Link="Models\Column.cs" />
		<Compile Include="..\LightningRevit_V2019\Models\ColumnDetail.cs" Link="Models\ColumnDetail.cs" />
